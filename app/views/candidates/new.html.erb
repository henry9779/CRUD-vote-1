<h1>新增候選人</h1>

<%= form_for @candidate do |f| %>
<%= f.label :name, '候選人姓名' %>
<%= f.text_field :name %><br/>
<%= f.label :party, '黨派' %>
<%= f.text_field :party %><br/>
<%= f.label :age, '年紀' %>
<%= f.text_field :age %><br/>
<%= f.label :politics, '政見' %>
<%= f.text_field :politics %><br/>
<%= f.submit '新增' %>
<% end %>